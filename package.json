    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "fast-glob": "^3.3.3",
    "globals": "^16.3.0",
    "mini-css-extract-plugin": "^2.9.4",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.39.1",
    "typescript-plugin-css-modules": "^5.2.0",
    "ultracite": "^5.1.2",
